FLASK_ENV=development # or production, testing
APP_SECRET_KEY="your_flask_application_secret_key_for_sessions_etc" # IMPORTANT: Change this!

# Flask secret key
APP_SECRET_KEY=your_secret_key_here

# Database connection string
DATABASE_URL=sqlite:///your_database.db
# Example for MySQL:
# DATABASE_URL=postgresql://user:password@localhost:5432/hr_hire_db  

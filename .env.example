RESUME_UPLOAD_FOLDER=uploads/resumes

# Email settings (if used)
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_USER=your_email@example.com
EMAIL_PASSWORD=your_email_password

# Other configuration variables as needed
# ...


